{
  "name": "geeky-punks-converter",
  "version": "1.0.0",
  "private": true,
  "description": "Geeky Punks converter",
  "scripts": {
    "desktop:dev": "neu run",
    "manual:distribute": "node scripts/distribute-manual.mjs",
    "desktop:build": "neu build --release && npm run manual:distribute",
    "desktop:mac": "neu build --release && npm run manual:distribute",
    "desktop:win": "neu build --release && npm run manual:distribute",
    "desktop:linux": "neu build --release && npm run manual:distribute",
    "web": "python3 -m http.server 8080"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^11.7.0"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.9",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2"
  }
}
